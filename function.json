{
    "scriptFile": "function_app.py",
    "entryPoint": "main",
    "bindings": [
        {
            "authLevel": "function",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": ["get", "post"],
            "route": "{*route}"
        },
        {
            "type": "http",
            "direction": "out",
            "name": "$return"
        }
    ]
}